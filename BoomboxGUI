	lagaudios = not lagaudios
	if lagaudios then
		lag.BorderColor3 = Color3.fromRGB(0, 255, 0)
	else
		lagaudios = false
		lag.BorderColor3 = Color3.fromRGB(255, 0, 0)
	end
end)

