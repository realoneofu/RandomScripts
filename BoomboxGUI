-- Gui to Lua
-- Version: 3.2
lagaudios = false
annoyaudios = false
duping = false


-- Instances:

local boomboxgui = Instance.new("ScreenGui")
local yea = Instance.new("Frame")
local maintext = Instance.new("TextLabel")
local playradio = Instance.new("TextButton")
